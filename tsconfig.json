{
	"compilerOptions": {
		"target": "ES2019",
		"module": "CommonJS",
		"outDir": "./dist/",
		"moduleResolution": "Node",
		"declaration": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"noImplicitAny": true
	},
	"include": ["./src"]
}
